<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <connectionStrings>
		<add name="EMarketContext" 
		     connectionString="Data source=(localdb)\mssqllocaldb;initial catalog=EMarket;integrated security=True"
		     providerName="System.Data.SqlClient"/>
    </connectionStrings>
	<appSettings>
		<add key="webpages:Version" value="3.0.0.0" />
		<add key="webpages:Enabled" value="false" />
		<add key="ClientValidationEnabled" value="true" />
		<add key="UnobtrusiveJavaScriptEnabled" value="true" />
		<add key="log4net.Config" value="log4net.config" />
		<add key="log4net.Config.Watch" value="false" />
		<add key ="ServiceAddress" value ="http://localhost:57465/{0}.svc"/>
	</appSettings>
</configuration>